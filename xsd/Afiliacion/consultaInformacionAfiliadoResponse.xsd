			<xsd:element name="fechaExpedicionCedulaAfiliado" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="idMunicipioExpedicionCedulaAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
