			<xsd:element name="informacionLaboralAfiliado" type="InformacionesLaboralesAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="cotizantes" type="ConsultaCotizantesAfiliacion" nillable="true" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	

	<xsd:complexType name="TipoInformacionConsultaAfiliado">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="descripcion" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="codigo" type="xsd:string" nillable="true" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InformacionLaboralAfiliado">
		<xsd:sequence>
			<xsd:element name="tipoAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="claseAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="direccionLaboraAfiliado" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="barrioLaboraAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="municipioLaboraAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="departamentoLaboraAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="tipoIdentificacionEmpresaAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="numeroIdentificacionEmpresaAfiliado" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="digitoVerificacionEmpresaAfiliado" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="razonSocialEmpresaAfiliado" type="xsd:string" nillable="true" minOccurs="0"/>
