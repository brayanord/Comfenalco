			<xsd:element name="departamentoExpedicionCedulaAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="madreCabezaDeHogarAfiliado" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="nivelEscolaridadAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="gradoEscolaridadAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
