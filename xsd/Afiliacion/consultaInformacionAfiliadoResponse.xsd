			<xsd:element name="descripcionTipoDiscapacidadAfiliado" type="xsd:string" nillable="true"/>
			<xsd:element name="porcentajeGradoDiscapacidadAfiliado"	type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="direccionResidenciaAfiliado" type="xsd:string" nillable="true" minOccurs="0"/>
			<xsd:element name="barrioResidenciaAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="municipioResidenciaAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
			<xsd:element name="departamentoResidenciaAfiliado" type="TipoInformacionConsultaAfiliado" nillable="true" minOccurs="0"/>
